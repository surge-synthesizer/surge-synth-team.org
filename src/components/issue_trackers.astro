---
import { getCollection } from "astro:content";

const {} = Astro.props;

const pages = await getCollection("pages");
pages.sort((a, b) => {
    return a.data.order - b.data.order;
});
---

<ul>
    {
        pages.map((page) =>
            page.data.order !== 0 ? (
                <li>
                    <a href={page.data.issue_tracker}>{page.data.title}</a>
                </li>
            ) : (
                ""
            ),
        )
    }
</ul>
